{
  "name": "functions",
  "version": "1.0.0",
  "description": "Firebase Functions for Travel App",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:ocr": "jest src/invoice-capture",
    "test:ocr:url": "jest src/invoice-capture/test-detection-url.test.ts",
    "test:watch": "jest --watch",
    "test:scanImage": "npx ts-node src/invoice-capture/test.ts",
    "test:gemini": "npx ts-node src/invoice-capture/test-gemini.ts",
    "debug:vision": "npx ts-node src/invoice-capture/debug-vision-api.ts"
  },
  "engines": {
    "node": "20"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/billing": "^5.0.1",
    "@google-cloud/vision": "^4.0.2",
    "@google/generative-ai": "^0.24.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "firebase-admin": "^13.3.0",
    "firebase-functions": "^6.3.2",
    "google-auth-library": "^9.6.3",
    "googleapis": "^131.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.15.0",
    "firebase-functions-test": "^3.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "private": true,
  "directories": {
    "lib": "lib"
  }
}
